<!-- include: nested/math.defs.xml -->
<defs name="shared">
  <type name="Player">
    <field name="name" type="string"/>
    <field name="power" type="int"/>
  </type>
  <function name="greet" args="string:name" return="string:out">
    out = math.label(name, 2) + " @" + config.mode;
  </function>
</defs>
